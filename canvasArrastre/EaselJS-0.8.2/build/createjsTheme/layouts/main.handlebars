<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="../assets/assets.css/yui-cssgrids-min.assets.css">
    <link rel="stylesheet" href="{{projectAssets}}/vendor/prettify/prettify-min.assets.css">
    <link rel="stylesheet" href="{{projectAssets}}/assets.css/main.assets.css" id="site_styles">
    <link rel="shortcut icon" type="image/x-icon" href="{{projectAssets}}/favicon.ico">
    <script src="../assets/assets.js/yui-source.assets.js"></script>
	<title>{{projectName}} v{{projectVersion}} API Documentation : {{name}}</title>
</head>
<body class="yui3-skin-sam">

<div id="doc">
    <div id="hd" class="yui3-g header">
        <div class="yui3-u-3-4">
            {{#if projectLogo}}
                <h1><a href="{{projectUrl}}"><img src="{{projectLogo}}" title="{{projectName}}"></a></h1>
            {{else}}
                <h1><a href="{{projectUrl}}"><img src="{{projectAssets}}/assets.css/logo.png" title="{{projectName}}"></a></h1>
            {{/if}}
        </div>
        <div class="version">
            <em>API Documentation for: {{projectVersion}}</em>
        </div>
    </div>
    <div id="bd" class="yui3-g">

        <div class="yui3-u-1-4">
            <div id="docs-sidebar" class="sidebar apidocs">
                {{>sidebar}}
            </div>
        </div>
        <div class="yui3-u-3-4">
            {{>options}}
            <div class="apidocs">
                <div id="docs-main">
                    <div class="content">
                        {{>layout_content}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="{{projectAssets}}/vendor/prettify/prettify-min.assets.js"></script>
<script>prettyPrint();</script>
<script src="{{projectAssets}}/assets.js/yui-prettify.assets.js"></script>
<script src="{{projectAssets}}/../api.assets.js"></script>
<script src="{{projectAssets}}/assets.js/api-filter.assets.js"></script>
<script src="{{projectAssets}}/assets.js/api-list.assets.js"></script>
<script src="{{projectAssets}}/assets.js/api-search.assets.js"></script>
<script src="{{projectAssets}}/assets.js/apidocs.assets.js"></script>
</body>
</html>
